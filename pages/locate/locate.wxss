/* pages/locate/locate.wxss */
.located {
  background-color: #f5f5f5;
  font-size: 28rpx;
}
.locate-title {
  background-color: #ebebeb;
  height: 60rpx;
  padding-left: 30rpx;
  line-height: 60rpx;
}
.locate-city {
  padding: 30rpx 0 0 25rpx;
}
.city {
  margin-right: 25rpx;
  margin-bottom: 30rpx;
  display: inline-block;
  background-color: white;
  width: 200rpx;
  height: 65rpx;
  color: #343434;
  line-height: 65rpx;
  text-align: center;
  border: 1rpx inset #e5e5e5;
}
.locate-city-box {
  padding-left: 25rpx;
}
.city-line {
  margin-right: 60rpx;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 28rpx;
  color: #353535;
  border-bottom: 1rpx solid #c9c9c9;
}
.locate-city-box .city-line:last-child {
  border: none;
}